  if test -n "$_subconfig_extract_result" ; then
    eval $_subconfig_extract_result
    echo "$as_me:$LINENO: result: $PTHREADS" >&5
echo "${ECHO_T}$PTHREADS" >&6
  else
    echo "$as_me:$LINENO: result: failed" >&5
echo "${ECHO_T}failed" >&6
    { { echo "$as_me:$LINENO: error: failed to extract PTHREADS variable value from $_subconfig_extract_file" >&5
echo "$as_me: error: failed to extract PTHREADS variable value from $_subconfig_extract_file" >&2;}
   { (exit 1); exit 1; }; }
  fi

